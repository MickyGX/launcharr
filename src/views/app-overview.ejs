  <% } %>
  <% if (appBaseId === 'jellyfin' || appBaseId === 'emby') { %>
    <script>
      window.<%= appBaseId === 'emby' ? 'EMBY_OVERVIEW_CONFIG' : 'JELLYFIN_OVERVIEW_CONFIG' %> = <%- JSON.stringify({
